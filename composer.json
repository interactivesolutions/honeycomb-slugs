{
    "name": "interactivesolutions/honeycomb-slugs",
    "description": "HoneyComb CMS package checks if URLs is not repeating and if so creates a slug",
    "keywords": ["honeycomb", "honey", "laravel", "slugs", "url", "url-validate", "validate"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mantas Gelčius",
            "email": "mantas.gelcius@gmail.com"
        },
        {
            "name": "Liutauras Kavaliauskas",
            "email": "liutauraskav@gmail.com"
        },
        {
            "name": "Jevgenij Dmitrijev",
            "email": "jd@interactivesolutions.lt"
        },
        {
            "name": "Vytautas Rimeikis",
            "email": "vytautas.rimeikis@gmail.com"
        }
    ],
    "repositories": [],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.3.*",
        "interactivesolutions/honeycomb-core": "~1.0.0"
    },
    "require-dev": {
        "brianium/paratest": "^0.14",
        "mockery/mockery": "^0.9",
        "orchestra/testbench-browser-kit": "^3.3",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "interactivesolutions\\honeycombslugs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "paratest -p4 --colors"
        ]
    },
    "config": {
        "secure-http": false,
        "platform": {
            "php": "7.1"
        }
    }
}
